public class Solution {
    public bool hasDuplicate(int[] nums) {
       return new HashSet<int>(nums).Count != nums.Length;
    }
}

//Alternate approach

public class Solution {
    public bool hasDuplicate(int[] nums) {
        var set = new HashSet<int>();
        foreach(int num in nums){
            if(set.Contains(num))
            return true;
            set.Add(num);
        }
        return false;
    }
}
